{
  "disabled": false,
  "bindings": [
    {
      "name": "nodekitten",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0/5 * * * * *"
    },
    {
      "name": "nodekittenInputBlob",
      "type": "blob",
      "path": "releases/{data}",
      "direction": "in"
    },
    {
      "name": "nodeKittenOutputBlob",
      "type": "blob",
      "path": "releases/{data}",
      "direction": "out"
    }
  ]
}
